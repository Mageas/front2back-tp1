{
  "resource_version": 13,
  "metadata": {
    "version": 3,
    "sources": [
      {
        "name": "default",
        "kind": "postgres",
        "tables": [
          {
            "table": {
              "name": "Users",
              "schema": "public"
            },
            "object_relationships": [
              {
                "name": "company",
                "using": {
                  "manual_configuration": {
                    "column_mapping": {
                      "company_id": "id"
                    },
                    "insertion_order": null,
                    "remote_table": {
                      "name": "company",
                      "schema": "public"
                    }
                  }
                }
              }
            ]
          },
          {
            "table": {
              "name": "company",
              "schema": "public"
            },
            "array_relationships": [
              {
                "name": "Users",
                "using": {
                  "foreign_key_constraint_on": {
                    "column": "company_id",
                    "table": {
                      "name": "Users",
                      "schema": "public"
                    }
                  }
                }
              },
              {
                "name": "products",
                "using": {
                  "foreign_key_constraint_on": {
                    "column": "company_id",
                    "table": {
                      "name": "product",
                      "schema": "public"
                    }
                  }
                }
              }
            ],
            "select_permissions": [
              {
                "role": "user",
                "permission": {
                  "columns": [
                    "id",
                    "name",
                    "created_at",
                    "updated_at"
                  ],
                  "filter": {
                    "Users": {
                      "id": {
                        "_eq": "X-Hasura-User-Id"
                      }
                    }
                  }
                },
                "comment": ""
              }
            ]
          },
          {
            "table": {
              "name": "product",
              "schema": "public"
            },
            "object_relationships": [
              {
                "name": "company",
                "using": {
                  "foreign_key_constraint_on": "company_id"
                }
              }
            ],
            "select_permissions": [
              {
                "role": "user",
                "permission": {
                  "columns": [
                    "company_id",
                    "id",
                    "quantity",
                    "comment",
                    "name",
                    "created_at",
                    "updated_at"
                  ],
                  "filter": {
                    "company": {
                      "Users": {
                        "id": {
                          "_eq": "X-Hasura-User-Id"
                        }
                      }
                    }
                  }
                },
                "comment": ""
              }
            ]
          }
        ],
        "configuration": {
          "connection_info": {
            "database_url": {
              "from_env": "HASURA_GRAPHQL_DATABASE_URL"
            },
            "isolation_level": "read-committed",
            "pool_settings": {
              "connection_lifetime": 600,
              "idle_timeout": 180,
              "max_connections": 50,
              "retries": 1
            },
            "use_prepared_statements": true
          }
        }
      }
    ]
  }
}